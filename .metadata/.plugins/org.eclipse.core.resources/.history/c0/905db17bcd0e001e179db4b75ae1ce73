
public class VariableType {
	
	public static void main(String[] args) {
		
		System.out.println("I'm goin to show my age");
		
		int age = 37;
		
		System.out.println(age);
		
		age = 40;
		
		System.out.println(age);
		
		age = 20 + 82;
		
		System.out.println("My age is :" + age);
		
		double salary = 30.02;
		
		System.out.println(salary);
		
		double ages = 30.02;
		
	}

}
